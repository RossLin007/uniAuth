        "@types/nodemailer": "^7.0.4",
        "@types/qrcode": "^1.5.6",
        "@types/uuid": "^11.0.0",
        "@typescript-eslint/eslint-plugin": "^8.18.1",
        "@typescript-eslint/parser": "^8.18.1",
        "eslint": "^9.17.0",
        "jsonwebtoken": "^9.0.3",
        "tsup": "^8.3.5",
        "tsx": "^4.19.2",
        "typescript": "^5.7.2",
        "vitest": "^2.1.8"
